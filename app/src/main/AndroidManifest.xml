<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.unipay.uni">

    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_uni"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_uni"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.qr.MiCodigoQr"
            android:exported="true" />
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="fullSensor"
            tools:replace="screenOrientation" />
        <activity
            android:name=".ui.qr.MiCodigoQrConMonto"
            android:exported="true" />
        <activity
            android:name=".ui.transferencias.Transfiere"
            android:exported="true" />
        <activity
            android:name=".ui.usuario.DatosUsuario"
            android:exported="true" />
        <activity
            android:name=".ui.principal.CambiarAlias"
            android:exported="true"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.CambiarAlias"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.resumen.ResumenTransaccion"
            android:exported="true"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.ResumenTransaccion"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.transferencias.Transferir"
            android:exported="true"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.Transferir"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.principal.CuentaOrigen"
            android:exported="true"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.CuentaOrigen"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.resumen.ResumenUnipay"
            android:exported="true"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.ResumenUnipay"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.permisos.OtorgarPermisos"
            android:exported="true"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.OtorgarPermisos"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.permisos.EnrollaUni"
            android:exported="true"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.EnrollaUni"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>