<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unipay.uni">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_uni"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_uni"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".ui.qr.MiCodigoQr"
            android:exported="true" />
        <activity
            android:name=".ui.qr.MiCodigoQrConMonto"
            android:exported="true" />
        <activity
            android:name=".Transfiere"
            android:exported="true" />
        <activity
            android:name=".ui.DatosUsuario"
            android:exported="true" />
        <activity
            android:name=".ui.principal.CambiarAlias"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.CambiarAlias"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.ResumenTransaccion"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.ResumenTransaccion"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.Transferir"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.Transferir"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.principal.CuentaOrigen"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.CuentaOrigen"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.ResumenUnipay"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.ResumenUnipay"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.OtorgarPermisos"
            android:exported="true"
            android:parentActivityName=".MainActivity">
            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.OtorgarPermisos"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.EnrollaUni"
            android:exported="true"

            android:parentActivityName=".MainActivity">
        <!-- The meta-data tag is required if you support API level 15 and lower -->
        <meta-data
            android:name="android.support.EnrollaUni"
            android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>