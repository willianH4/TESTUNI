<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unipay.uni">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_uni"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_uni"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".DatosUsuario"
            android:exported="true" />
        <activity
            android:name=".ui.CambiarAlias"
            android:exported="true" />
        <activity
            android:name=".ui.ResumenTransaccion"
            android:exported="true" />
        <activity
            android:name=".ui.Transferir"
            android:exported="true" />
        <activity
            android:name=".ui.CuentaOrigen"
            android:exported="true" />
        <activity
            android:name=".ui.ResumenUnipay"
            android:exported="true" />
        <activity
            android:name=".ui.OtorgarPermisos"
            android:exported="true" />
        <activity
            android:name=".ui.EnrollaUni"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>